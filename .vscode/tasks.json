{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildn",
            "type": "shell",
            "options": { "statusbar": {"hide": true}},
            "command": "set -a;source ./build.env; dotnet build ./src/CASTools/S4CASTools.NET.csproj /property:GenerateFullPaths=true /consoleloggerparameters:NoSummary -f:net6.0-windows",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "options": { "statusbar": {"hide": false}},
            "type": "shell",
            "command": "./release.sh",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "wine üç∑",
            "command": "wine",
            "type": "process",
            "options": {
                "cwd": "${workspaceFolder}/src/CASTools/bin/Debug/net6.0-windows/win-x64/",
                "env": {
                    "WINEDEBUG":"-all"
                }
            },
            "dependsOn":[
                "buildn"
            ],
            "args":[
                "S4CASTools.exe"
            ]
        }
    
    ]
}